{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:local": "vite --host 0.0.0.0",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "deploy": "npm run build && firebase deploy --only hosting",
    "deploy:all": "npm run build && firebase deploy",
    "deploy:script": "bash scripts/deploy.sh",
    "deploy:windows": "scripts\\deploy.bat",
    "firebase:init": "firebase init",
    "firebase:login": "firebase login",
    "firebase:serve": "firebase serve --only hosting",
    "import:csv": "node scripts/import-csv-data.js",
    "cap:add:android": "npx cap add android",
    "cap:add:ios": "npx cap add ios",
    "cap:sync": "npx cap sync",
    "cap:open:android": "npx cap open android",
    "cap:open:ios": "npx cap open ios",
    "cap:run:android": "npx cap run android",
    "cap:run:ios": "npx cap run ios",
    "android": "npm run build && npx cap sync android && npx cap open android",
    "ios": "npm run build && npx cap sync ios && npx cap open ios",
    "android:build": "npm run build && npx cap sync android",
    "ios:build": "npm run build && npx cap sync ios",
    "android:run": "npm run android:build && npx cap run android",
    "ios:run": "npm run ios:build && npx cap run ios",
    "build:web": "bash scripts/build.sh web",
    "build:android": "bash scripts/build.sh android",
    "build:ios": "bash scripts/build.sh ios",
    "build:all": "bash scripts/build.sh all",
    "check:env": "bash scripts/build.sh check",
    "build:web:win": "scripts\\build.bat web",
    "build:android:win": "scripts\\build.bat android",
    "build:ios:win": "scripts\\build.bat ios",
    "build:all:win": "scripts\\build.bat all",
    "check:env:win": "scripts\\build.bat check"
  },
  "dependencies": {
    "@capacitor/android": "^7.0.2",
    "@capacitor/app": "^7.1.0",
    "@capacitor/camera": "^7.0.2",
    "@capacitor/core": "^7.0.2",
    "@capacitor/device": "^7.0.2",
    "@capacitor/filesystem": "^7.0.2",
    "@capacitor/ios": "^7.0.2",
    "@capacitor/keyboard": "^7.0.2",
    "@capacitor/network": "^7.0.2",
    "@capacitor/preferences": "^7.0.2",
    "@capacitor/push-notifications": "^7.0.2",
    "@capacitor/splash-screen": "^7.0.2",
    "@capacitor/status-bar": "^7.0.2",
    "@emoji-mart/data": "^1.2.1",
    "@emoji-mart/react": "^1.1.1",
    "@hookform/resolvers": "^3.1.1",
    "@react-spring/web": "^9.7.3",
    "@reduxjs/toolkit": "^1.9.5",
    "@tanstack/react-query": "^4.29.19",
    "firebase": "^10.7.1",
    "lodash.debounce": "^4.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.1",
    "react-icons": "^4.10.1",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.14.0",
    "react-spinners": "^0.13.8",
    "react-toastify": "^9.1.3",
    "redux-persist": "^6.0.0",
    "uuid": "^9.0.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.0.2",
    "@types/lodash.debounce": "^4.0.7",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.2",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "cypress": "^12.17.2",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-plugin-pwa": "^0.16.4"
  }
}
